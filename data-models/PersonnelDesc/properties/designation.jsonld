            "rdfs:label": "designation",
            "adex:domainIncludes": [
                {
                    "@id": "adex:PersonnelDesc"
                },
                {
                    "@id": "adex:Transporter"
                },
                {
                    "@id": "adex:AgriExpert"
                },
                {
                    "@id": "adex:ExtensionWorker"
                },
                {
                    "@id": "adex:AgriResearcher"
                }
            ],
            "adex:rangeIncludes": [
                {
                    "@id": "adex:Text"
                },
                {
                    "@id": "adex:ValueDescriptor"
                }
            ],
            "rdfs:isDefinedBy": {
                "@id": "adex:"
            }
        }
    ]
}