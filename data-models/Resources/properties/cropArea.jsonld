{
    "@context": {
        "rdfs": "http://www.w3.org/2000/01/rdf-schema#",
        "owl": "http://www.w3.org/2002/07/owl#",
        "rdf": "http://www.w3.org/1999/02/22-rdf-syntax-ns#",
        "iudx": "https://voc.iudx.org.in/",
        "adex": "https://agrijson.adex.org.in/"
    },
    "@graph": [
        {
            "@id": "adex:cropArea",
            "@type": [
                "adex:QuantitativeProperty"
            ],
            "rdfs:comment": "Area of the agricultural land used for cultivation, i.e, the arable land area.",
            "rdfs:label": "cropArea",
            "adex:domainIncludes": [
                {
                    "@id": "adex:LandParcel"
                },
                {
                    "@id": "adex:CropCondition"
                }
            ],
            "adex:rangeIncludes": [
                {
                    "@id": "adex:Number"
                },
                {
                    "@id": "adex:ValueDescriptor"
                }
            ],
            "rdfs:isDefinedBy": {
                "@id": "adex:"
            }
        }
    ]
}