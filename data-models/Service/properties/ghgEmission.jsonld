{
    "@context": {
        "rdfs": "http://www.w3.org/2000/01/rdf-schema#",
        "owl": "http://www.w3.org/2002/07/owl#",
        "rdf": "http://www.w3.org/1999/02/22-rdf-syntax-ns#",
        "iudx": "https://voc.iudx.org.in/",
        "adex": "https://agrijson.adex.org/"
    },
    "@graph": [
        {
            "@id": "adex:ghgEmission",
            "@type": [
                "adex:QuantitativeProperty"
            ],
            "rdfs:comment": "The amount of greenhouse gas(es) emitted during the reporting period. In case of multiple gases, it is an array of gas emission measures in the order of gases mentioned in 'ghgName'.",
            "rdfs:label": "ghgEmission",
            "rdfs:isDefinedBy": {
                "@id": "adex:"
            },
            "adex:domainIncludes": [
                {
                    "@id": "adex:GhgReport"
                }
            ],
            "adex:rangeIncludes": [
                {
                    "@id": "adex:Number"
                },
                {
                    "@id": "adex:NumericArray"
                },
                {
                    "@id": "adex:ValueDescriptor"
                }
            ]
        }
    ]
}