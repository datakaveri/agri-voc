{
    "@context": {
        "rdfs": "http://www.w3.org/2000/01/rdf-schema#",
        "owl": "http://www.w3.org/2002/07/owl#",
        "rdf": "http://www.w3.org/1999/02/22-rdf-syntax-ns#",
        "iudx": "https://voc.iudx.org.in/",
        "adex": "https://agrijson.org/"
    },
    "@graph": [
        {
            "@id": "adex:ghgScope",
            "@type": [
                "adex:TextProperty"
            ],
            "rdfs:comment": "Scope of the emission being reported as per GHG Protocol - Scope 1, Scope 2, Scope 3 as per the India GHG Program https://www.indiaghgp.org/explaining-scope-1-2-3",
            "rdfs:label": "ghgScope",
            "rdfs:isDefinedBy": {
                "@id": "adex:"
            },
            "adex:domainIncludes": [
                {
                    "@id": "adex:GhgReport"
                }
            ],
            "adex:rangeIncludes": [
                {
                    "@container": "@list",
                    "@id": "adex:Text"
                },
                {
                    "@id": "adex:ValueDescriptor"
                }
            ]
        }
    ]
}