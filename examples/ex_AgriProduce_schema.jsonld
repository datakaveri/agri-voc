{
    "@context": "https://agrijson.adex.org.in",
    "type": [
        "adex:AgriProduce",
        "adex:Schema"
    ],
    "id": "https://agrijson.adex.org.in/AgriProduce",
    "schema": {
        "title": "ADEX AgriProduce Data Model",
        "type": "object",
        "properties": {
            "type": {
                "type": "array",
                "items": [
                    {
                        "type": "string",
                        "description": "ADEX Data Model name. It has to be AgriProduce"
                    },
                    {
                        "type": "string",
                        "description": "ADEX Entity type. It has to be Resource"
                    }
                ]
            },
            "id": {
                  "type": "string",
                  "description": "Unique ADEX ID for the resource."
            },
            "minimumPrice": {
                "type": "number",
                "description": "The minimum price of the product/commodity/service corresponding to this observation.",
                "unitCode": "INR",
                "unitText": "Indian Rupee"
            },
            "commodityVarietyCode": {
                "type": "string",
                "description": "The unique code assigned to the variety of a commodity corresponding to this observation."
            },
            "arrivalQuantity": {
                "type": "number",
                "description": "The quantity of commodity arrived at the market corresponding to this observation.",
                "unitCode": "KGM",
                "unitText": "kilogram"
            },
            "commodityCodeICC": {
                "type": "string",
                "description": "The unique code assigned to the commodity corresponding to this observation as given by the ICC (Indicative Crop Classification)."
            },
            "modalPrice": {
                "type": "number",
                "description": "The most frequent pricing/modal price of the product/commodity/service corresponding to this observation.",
                "unitCode": "INR",
                "unitText": "Indian Rupee"
            },
            "maximumPrice": {
                "type": "number",
                "description": "The maximum price of the product/commodity/service corresponding to this observation.",
                "unitCode": "INR",
                "unitText": "Indian Rupee"
            },
            "commodityNameICC": {
                "type": "string",
                "description": "The name of the commodity corresponding to this observation as given by the ICC (Indicative Crop Classification)."
            },
            "commodityVarietyName": {
                "type": "string",
                "description": "Variety name of the commodity corresponding to this observation."
            },
            "tradedQuantity": {
                "type": "number",
                "description": "The total quantity of commodity that has been traded in the market corresponding to this observation.",
                "unitCode": "KGM",
                "unitText": "kilogram"
            },
            "commodityName": {
                "type": "string",
                "description": "The local name of the commodity corresponding to this observation."
            },
            "observationDateTime": {
                "type": "string",
                "description": "Last reported time of observation.",
                "format": "date-time"
            }
        }
    }
}